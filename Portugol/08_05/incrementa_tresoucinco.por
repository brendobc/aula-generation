programa
{
	funcao inicio()
	{
		inteiro num=233

		escreva(num)
		faca
		{
			se(num>=300 e num<=400){num+=3}
			senao{num+=5}
			escreva("\n", num)
		} enquanto(num <= 456)
	}
}
