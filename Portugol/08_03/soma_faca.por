programa
{
	funcao inicio()
	{
		inteiro num = 233
		
		faca
		{
			escreva("\n", num)
			
			se (num >= 300 e num <= 400)
			{
				num += 3
			}
			senao
			{
				num += 5
			}
		} enquanto (num >= 233 e num <= 456)

	}
}
